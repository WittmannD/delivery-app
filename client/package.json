{
  "name": "client",
  "version": "0.1.0",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@hookform/resolvers": "^2.9.5",
    "@react-google-maps/api": "^2.12.0",
    "@reduxjs/toolkit": "^1.8.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.27.2",
    "flowbite": "^1.4.7",
    "flowbite-react": "^0.1.3",
    "joi": "^17.6.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.33.1",
    "react-hot-toast": "^2.3.0",
    "react-number-format": "^4.9.3",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:prod": "dotenv -e .env.production.local react-scripts build && shx rm -rf ../server/build && shx cp -r ./build ../server/build",
    "build:dev": "dotenv -e .env.development.local react-scripts build && shx rm -rf ../server/build && shx cp -r ./build ../server/build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended",
      "plugin:tailwindcss/recommended",
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "prettier/prettier": [
        "warn",
        {
          "singleQuote": false,
          "endOfLine": "auto"
        }
      ],
      "react/react-in-jsx-scope": "off",
      "jsx-a11y/anchor-is-valid": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tailwindcss": "^3.6.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "shx": "^0.3.4",
    "tailwindcss": "^3.1.6"
  }
}
